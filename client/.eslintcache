ype=\"submit\" className=\"btn btn-primary my-1\" />\r\n                <Link to=\"/dashboard\" className=\"btn btn-light my-1\">Go Back</Link>\r\n            </form>\r\n        </Fragment>\r\n    )\r\n}\r\n\r\nEditProfile.propTypes = {\r\n    createProfile: PropTypes.func.isRequired,\r\n    profile: PropTypes.object.isRequired,\r\n    getCurrentProfile: PropTypes.func.isRequired,\r\n}\r\n\r\nconst mapStateToProps = state => ({\r\n    profile: state.profile\r\n})\r\n\r\nexport default connect(mapStateToProps, { createProfile, getCurrentProfile })(withRouter(EditProfile));\r\n","C:\\Users\\Marenda\\Documents\\code\\devconnector\\client\\src\\components\\profile-forms\\AddExperience.js",[],"C:\\Users\\Marenda\\Documents\\code\\devconnector\\client\\src\\components\\profile-forms\\AddEducation.js",[],{"ruleId":"114","replacedBy":"115"},{"ruleId":"116","replacedBy":"117"},{"ruleId":"118","severity":1,"message":"119","line":5,"column":1,"nodeType":"120","endLine":15,"endColumn":2},{"ruleId":"118","severity":1,"message":"119","line":18,"column":1,"nodeType":"120","endLine":51,"endColumn":2},{"ruleId":"121","severity":1,"message":"122","line":5,"column":8,"nodeType":"123","messageId":"124","endLine":5,"endColumn":15},{"ruleId":"125","severity":1,"message":"126","line":13,"column":8,"nodeType":"127","endLine":13,"endColumn":10,"suggestions":"128"},{"ruleId":"118","severity":1,"message":"119","line":11,"column":1,"nodeType":"120","endLine":38,"endColumn":2},{"ruleId":"125","severity":1,"message":"129","line":25,"column":8,"nodeType":"127","endLine":25,"endColumn":17,"suggestions":"130"},"no-native-reassign",["131"],"no-negated-in-lhs",["132"],"import/no-anonymous-default-export","Unexpected default export of anonymous function","ExportDefaultDeclaration","no-unused-vars","'Spinner' is defined but never used.","Identifier","unusedVar","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'getCurrentProfile'. Either include it or remove the dependency a